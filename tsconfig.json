{
	"compilerOptions": {
		"watch": false,
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"lib": [
			"es6",
            "dom"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"noUnusedLocals": false,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"strictNullChecks": true,
		"noUnusedParameters": false,
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}